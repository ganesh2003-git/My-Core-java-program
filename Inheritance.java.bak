class Cal
{
	void add(int a,int b){
	int c=a+b;
	System.out.println("addition a&b"+c);
	}
	
	void sub(int a, int b){
	int c=a-b;
	System.out.println("sub a&b"+c);
}
}

class Avdcal extends Cal
{
	 void mul(int a,int b){
	 int c=a*b;
	 System.out.println("mul a&b"+c);
	 } 


void div(double a,double b){
double c=a/b;
System.out.println("dev a&b"+c);
}
}

class Mulcal extends Avdcal
{
	double power (int a,int b){
	return Math.pow(a,b);
}
}

class Inheritance 
{
	public static void main(String[] args) 
	{
		Mulcal m1=new Mulcal();
		m1.add(2,2);
		m1.sub(4-2);
		
		System.out.println(m1.power(10,2));
	}
}
